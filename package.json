{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "@types/jest": "^29.5.3",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "name": "api2ai",
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "core",
    "demo",
    "packages/*"
  ],
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "yarn run format"
    ],
    "*.md": "prettier --write"
  }
}
